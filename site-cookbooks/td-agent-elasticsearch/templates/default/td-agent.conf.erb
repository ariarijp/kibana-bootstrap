<source>
  type tail
  format apache
  path /var/log/httpd/access_log
  tag web.apache.access
</source>

<match *.apache.access>
  index_name adminpack
  type_name apache
  type elasticsearch
  include_tag_key true
  tag_key @log_name
  host 192.168.50.101
  port 9200
  logstash_format true
  flush_interval 10s
</match> 
